{
    "name": "rhino-ui",
    "version": "0.0.1",
    "private": true,
    "description": "Frontend of Rhino Load Testing Platform",
    "main": "index.js",
    "scripts": {
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build",
        "test": "jest",
        "test:watch": "jest --watch",
        "start": "webpack-dev-server --client-log-level debug --mode development --hot --progress --color --port 3000 --host 0.0.0.0 --open",
        "build": "webpack -p --progress --colors --env.TSSTRICT",
        "tslint": "tslint 'src/**/*.{ts,tsx}'",
        "tslint:fix": "npm run tslint -- --fix",
        "tslint:check": "tslint-config-prettier-check ./tslint.json",
        "prettier": "prettier --write --config ./.prettierrc './src/**/*.{ts,tsx}' './__test__/**/*.{ts,tsx}'"
    },
    "devDependencies": {
        "@babel/core": "^7.4.0",
        "@types/classnames": "^2.2.7",
        "@types/enzyme": "^3.9.1",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/history": "^4.7.2",
        "@types/jest": "^24.0.11",
        "@types/node": "^10.14.1",
        "@types/react": "^16.8.2",
        "@types/react-dom": "^16.8.3",
        "@types/react-redux": "^7.0.5",
        "@types/react-router-dom": "^4.3.1",
        "@types/redux-actions": "2.3.1",
        "@types/redux-logger": "^3.0.7",
        "@types/redux-mock-store": "^1.0.0",
        "@types/webpack": "^4.4.26",
        "@types/webpack-env": "1.13.6",
        "acorn": "^6.1.1",
        "awesome-typescript-loader": "^5.2.1",
        "babel-loader": "^8.0.5",
        "css-loader": "^2.1.0",
        "css-modules-typescript-loader": "^1.1.1",
        "enzyme": "^3.9.0",
        "enzyme-adapter-react-16": "^1.11.2",
        "enzyme-to-json": "^3.3.5",
        "file-loader": "^3.0.1",
        "gh-pages": "^2.0.1",
        "html-loader": "^1.0.0-alpha.0",
        "html-webpack-plugin": "^4.0.0-alpha",
        "husky": "^1.3.1",
        "jest": "^24.5.0",
        "mini-css-extract-plugin": "^0.5.0",
        "postcss": "^7.0.13",
        "postcss-browser-reporter": "^0.5.0",
        "postcss-import": "^12.0.1",
        "postcss-loader": "^3.0.0",
        "postcss-preset-env": "^6.5.0",
        "postcss-reporter": "^6.0.1",
        "postcss-url": "^8.0.0",
        "prettier": "^1.16.4",
        "react-hot-loader": "^4.6.3",
        "redux-devtools-extension": "^2.13.7",
        "redux-mock-store": "^1.5.3",
        "style-loader": "^0.23.1",
        "ts-jest": "^24.0.0",
        "ts-loader": "^5.3.3",
        "tslint": "^5.14.0",
        "tslint-config-prettier": "^1.18.0",
        "tslint-plugin-prettier": "^2.0.1",
        "tslint-react": "^3.6.0",
        "typescript": "^3.3.4000",
        "url-loader": "^1.1.2",
        "util": "^0.11.1",
        "webpack": "^4.28.4",
        "webpack-cleanup-plugin": "^0.5.1",
        "webpack-cli": "^3.3.0",
        "webpack-dev-server": "^3.1.14"
    },
    "dependencies": {
        "classnames": "^2.2.6",
        "react": "^16.8.1",
        "react-bootstrap": "^1.0.0-beta.6",
        "react-dom": "^16.8.1",
        "react-redux": "^6.0.0",
        "react-router-dom": "^5.0.0",
        "reactn": "^2.1.4",
        "redux": "^4.0.1",
        "redux-actions": "^2.6.4",
        "redux-logger": "^3.0.6",
        "redux-starter-kit": "^0.4.3"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run prettier",
            "pre-push": "npm run build && npm run test && npm run tslint"
        }
    }
}
